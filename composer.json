{
  "name": "islandora/openseadragon",
  "description": "OpenSeadragon",
  "type": "drupal-module",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "keywords": ["Drupal", "Islandora", "OpenSeadragon"],
  "license": "GPL-2.0+",
  "homepage": "https://github.com/Islandora/openseadragon",
  "support": {
    "issues": "https://github.com/Islandora/documentation/issues"
  },
  "repositories": [
      {
          "type": "composer",
          "url": "https://asset-packagist.org"
      }
  ],
  "require": {
    "drupal/token": "^1.3",
    "npm-asset/openseadragon": "^2.4"
  },
  "authors": [
    {
      "name": "Islandora Foundation",
      "email": "community@islandora.ca",
      "role": "Owner"
    },
    {
      "name": "Daniel Lamb",
      "email": "dlamb@islandora.ca",
      "role": "Maintainer"
    },
    {
      "name": "Jared Whiklo",
      "email": "jwhiklo@gmail.com",
      "role": "Maintainer"
    }
  ],
  "extra": {
      "installer-types": [
          "npm-asset"
      ],
      "installer-paths": {
          "web/libraries/{$name}": ["type:npm-asset"]
      }
  }
}
